# UltraRAG MCP Authentication Configuration
# Copy this file to .env and modify the values as needed

# Enable/disable authentication for MCP servers
ENABLE_AUTH=false

# Database configuration for authentication
DATABASE_URL=postgresql://user:password@localhost:5432/strapi

# JWT secret for token validation
JWT_SECRET=your-secret-key-change-this-in-production

# API Key header name
API_KEY_HEADER=X-API-Key

# Log level for authentication
AUTH_LOG_LEVEL=INFO
